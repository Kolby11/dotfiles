{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "palette": {
        "color_1": "#80d5d3",
        "color_2": "#b0ccca",
        "color_3": "#b2c8e8",
        "color_4": "#00504f",
        "color_5": "#1a2120",
        "color_fg": "#003736",
        "color_fg_dark": "#dde4e3",
        "color_accent": "#b2c8e8",
        "color_warn": "#93000a",
        "color_error": "#ffb4ab",
        "color_info": "#b0ccca",
        "color_success": "#80d5d3",
        "color_sep": "#b0ccca",
        "color_label": "#b2c8e8"
    },
    "blocks": [
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "background": "p:color_1",
                    "foreground": "p:color_fg",
                    "leading_diamond": "\u256d\u2500\ue0b2",
                    "options": {
                        "alpine": "\uf300",
                        "arch": "\uf303",
                        "centos": "\uf304",
                        "debian": "\uf306",
                        "elementary": "\uf309",
                        "fedora": "\uf30a",
                        "gentoo": "\uf30d",
                        "linux": "\ue712",
                        "macos": "\ue711",
                        "manjaro": "\uf312",
                        "nixos": "\uf313",
                        "mint": "\uf30f",
                        "opensuse": "\uf314",
                        "raspbian": "\uf315",
                        "ubuntu": "\uf31c",
                        "windows": "\ue70f"
                    },
                    "style": "diamond",
                    "template": " {{ if .WSL }}\ue712 on {{ end }}{{ .Icon }}  ",
                    "type": "os"
                },
                {
                    "background": "p:color_3",
                    "foreground": "p:color_fg",
                    "powerline_symbol": "\ue0b0",
                    "options": {
                        "folder_icon": "\uf07b",
                        "style": "folder"
                    },
                    "style": "powerline",
                    "template": " {{ .Path }} ",
                    "type": "path"
                },
                {
                    "background": "p:color_4",
                    "foreground": "p:color_accent",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "foreground_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
                        "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
                        "{{ if gt .Behind 0 }}#B388FF{{ end }}"
                    ],
                    "options": {
                        "branch_template": "{{ trunc 25 .Branch }}",
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "branch_icon": "\ue725 ",
                        "cherry_pick_icon": "\ue29b ",
                        "commit_icon": "\uf417 ",
                        "merge_icon": "\ue727 ",
                        "no_commits_icon": "\uf0c3 ",
                        "rebase_icon": "\ue728 ",
                        "revert_icon": "\uf0e2 ",
                        "tag_icon": "\uf412 "
                    },
                    "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
                    "trailing_diamond": "\ue0b4",
                    "type": "git"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "foreground": "p:color_success",
                    "style": "diamond",
                    "template": "<p:color_sep> | </><p:color_label>RAM:</> {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB",
                    "type": "sysinfo"
                },
                {
                    "foreground": "p:color_success",
                    "options": {
                        "fetch_version": true
                    },
                    "style": "powerline",
                    "template": "<p:color_sep> | </><p:color_label>\ue718</> {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
                    "type": "node"
                },
                {
                    "type": "php",
                    "style": "powerline",
                    "foreground": "p:color_success",
                    "template": "<p:color_sep> | </><p:color_label>\ue73d</> {{ .Full }}"
                },
                {
                    "type": "npm",
                    "style": "powerline",
                    "foreground": "p:color_success",
                    "template": "<p:color_sep> | </><p:color_label>\ue71e </> {{ .Full }}"
                },
                {
                    "type": "sysinfo",
                    "style": "powerline",
                    "foreground": "p:color_success",
                    "template": "<> </>"
                },
                {
                    "background": "p:color_5",
                    "foreground": "p:color_fg_dark",
                    "leading_diamond": "\ue0b2",
                    "trailing_diamond": "\ue0b0",
                    "options": {
                        "style": "austin",
                        "threshold": 150
                    },
                    "style": "diamond",
                    "template": "\u231b  {{ .FormattedMs }} ",
                    "type": "executiontime"
                },
                {
                    "background": "p:color_5",
                    "foreground": "p:color_fg_dark",
                    "leading_diamond": "\ue0b2",
                    "options": {
                        "time_format": "15:04:05"
                    },
                    "style": "diamond",
                    "template": " {{ .CurrentDate | date .Format }} ",
                    "trailing_diamond": "\ue0b0",
                    "type": "time"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "p:color_1",
                    "style": "plain",
                    "template": "\u2570\u2500>",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 4
}